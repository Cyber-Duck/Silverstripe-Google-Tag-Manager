{
    "name": "cyber-duck/silverstripe-google-tag-manager",
    "description": "Add Tag Manager data layer, events, and ecommerce support for SilverStripe",
    "type": "silverstripe-vendormodule",
    "homepage": "http://github.com/cyber-duck/silverstripe-google-tag-manager",
    "keywords": [
        "silverstripe",
        "silverstripe 4",
        "gtm",
        "google tag manager",
        "website tracking",
        "google analytics"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Mc Cormack",
            "email": "andrewm@cyber-duck.co.uk",
            "homepage": "https://github.com/Andrew-Mc-Cormack"
        }
    ],
    "support": {
        "issues": "http://github.com/cyber-duck/silverstripe-google-tag-manager/issues"
    },
    "require": {
        "php": ">=5.6",
        "silverstripe/framework": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "CyberDuck\\GTM\\": "src/",
            "CyberDuck\\GTM\\Tests\\": "tests/"
        }
    },
    "extra": {
        "installer-name": "gtm",
        "expose": [
            "css",
            "js"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}